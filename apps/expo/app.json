{
  "expo": {
    "name": "mini-trello",
    "slug": "solito-blank",
    "version": "1.0.0",
    "scheme": "mini-trello",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
