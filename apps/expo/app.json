    "ios": {
      "bundleIdentifier": "com.ivangoro.minitrello",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.ivan_goro.minitrello"
    },
    "extra": {
      "eas": {
